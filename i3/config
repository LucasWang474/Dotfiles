# Section: Bar
########################################################################################################################
########################################################################################################################


########################################################################################################################
### i3 bar
# Start i3bar to display a workspace bar (plus the system information i3status finds out, if available)
bar {
        status_command i3status -c $HOME/.config/i3/i3status.conf
        #status_command i3status

        #status_command i3blocks -c $HOME/.config/i3blocks/i3blocks.conf

        position top

        i3bar_command i3bar --transparency

        # Syntax: tray_output none|primary|<output>
        #tray_output none
        tray_output primary

        # Material Teal
        colors {
            background $barcolor
            separator  #282828
            #statusline #ffffff

            # class                 border       background    text           indicator
            focused_workspace       $fws         $fws          $textcolor     $bgcolor
            #active_workspace        $ibgcolor    $ibgcolor     $itextcolor    $ibgcolcor  
            inactive_workspace      $ibgcolor    $ibgcolor     $itextcolor    $ibgcolor
            urgent_workspace        $ubgcolor    $ubgcolor     $textcolor     $bgcolor
            #binding_mode            $ubgcolor    $ubgcolor     $textcolor     $bgcolor
        }
}
