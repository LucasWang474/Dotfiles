########################################################################################################################





########################################################################################################################
########################################################################################################################
##### Section: Monitors
########################################################################################################################
########################################################################################################################

########################################################################################################################
# Set monitor names
set $eDP eDP # My laptop screen
set $HDMI1 HDMI-A-0 # My SAMSUNG screen
set $firstMonitor $eDP
set $secondMonitor $HDMI1

########################################################################################################################
### Deal with screen tearing
# Since this might not always work, you may try this:
################################################
#vim /etc/X11/xorg.conf.d/20-amdgpu.conf
#Section "Device"
#    Identifier "AMD"
#    Driver "amdgpu"
#    Option "TearFree" "true"
#EndSection
################################################

exec_always --no-startup-id xrandr --output $firstMonitor --set TearFree on
exec_always --no-startup-id xrandr --output $secondMonitor --set TearFree on
########################################################################################################################

########################################################################################################################
set $screen_scripts $HOME/.config/screenlayout
#exec $screen_scripts/init.sh
#exec $screen_scripts/primary.sh
exec --no-startup-id $screen_scripts/vertical.sh

bindsym $mod+i mode "$init"
set $init (p)rimary, (o)ther, (v)ertiacal, (h)orizontal, (d)uplicate
mode "$init" {
    bindsym p exec $screen_scripts/primary.sh; exec nitrogen --restore
    bindsym o exec $screen_scripts/other.sh; exec nitrogen --restore
    bindsym v exec $screen_scripts/vertical.sh; exec nitrogen --restore
    bindsym h exec $screen_scripts/horizontal.sh; exec nitrogen --restore
    bindsym d exec $screen_scripts/duplicate.sh; exec nitrogen --restore
    
    bindsym Return mode "default"
    bindsym Escape mode "default"
}
