##### Section: screenshot
########################################################################################################################
########################################################################################################################

bindsym $mod+c mode "$screenshot"

set $screenshot (alt+a)rea, (alt+s)ingle screen, a(alt+l) screens, save (Shift+s)ingle screen, save a(Shift+l)l screens  
mode "$screenshot" {
    # Some tools (such as import or xdotool) might be unable to run 
    # upon a KeyPress event, because the keyboard/pointer is still grabbed. 
    # For these situations, the --release flag can be used, which will 
    # execute the command after the keys have been released.

    bindsym $alt+a --release exec flameshot gui
    
    bindsym $alt+s --release exec flameshot screen -c
    bindsym Shift+s --release exec flameshot screen -p $HOME/Pictures/
    
    bindsym $alt+l --release exec flameshot full -c
    bindsym Shift+l --release exec flameshot full -p $HOME/Pictures/
    
    bindsym Return mode "default"
    bindsym Escape mode "default"
}

